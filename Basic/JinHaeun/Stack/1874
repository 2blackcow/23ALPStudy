#include <iostream>
#include <stack>
#include <vector>
using namespace std;

int main()
{
    int n;
    cin >> n;

    stack<int> stk;
    vector<char> op;

    int a;
    int cnt = 1;

    for (int i = 0; i < n; i++)
    {
        cin >> a;

        while (cnt <= a)
        {
            stk.push(cnt);
            cnt++;
            op.push_back('+');
        }

        if (stk.top() == a)
        {
            stk.pop();
            op.push_back('-');
        }
        else
        {
            cout << "NO";
            return 0;
        }
    }

    for (int i = 0; i < op.size(); i++)
        cout << op[i] << '\n';
    

    return 0;
}
